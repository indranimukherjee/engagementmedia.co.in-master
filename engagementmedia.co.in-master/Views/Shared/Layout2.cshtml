<!DOCTYPE html>

<html>
    <head>
         @if (ViewBag.Title == "Engagement Media")
    {
        <title>@ViewBag.Title</title>
    }
    else
    {
        <title>@ViewBag.Title | Engagement Media</title>
    }
        <meta charset="utf-8" />
        <title>@ViewBag.Title</title>
         <link rel="icon" href="../../Content/images/favicon.ico" type="image/x-icon" />
      <link href="@Url.Content("~/Content/LandingStyle.css")" rel="stylesheet" type="text/css" />
        <link href="@Url.Content("~/Content/Footer.css")" rel="stylesheet" type="text/css" />
        <link href="@Url.Content("~/Content/Header.css")" rel="stylesheet" type="text/css" />
        <link href="@Url.Content("~/Content/menu.css")" rel="stylesheet" type="text/css" />
        <!--[if lt IE 9]><script src="//html5shim.googlecode.com/svn/trunk/html5.js"></script><![endif]-->
        <link rel="stylesheet" href="@Url.Content("~/Content/rv.css")" type="text/css" media="screen">
         <link rel="stylesheet" href="@Url.Content("~/Content/font-awesome-4.1.0/css/font-awesome.min.css")" type="text/css" media="screen">
        @*<script src="@Url.Content("~/Scripts/jquery-1.9.1.min.js")" type="text/javascript"></script>*@
        <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
         <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/jquery.easing-sooper.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/jquery.sooperfish.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
        <script type="text/javascript" src="@Url.Content("~/Scripts/jquery.easing.1.3.js")"></script>
        <script type="text/javascript" src="@Url.Content("~/Scripts/jquery.themepunch.plugins.min.js")"></script>
        <script type="text/javascript" src="@Url.Content("~/Scripts/jquery.themepunch.revolution.min.js")"></script>
        <script type="text/javascript">
            $(document).ready(function () {
                $('ul.sf-menu').sooperfish();
            });
        </script>
    </head>
<body>
    <div class="main">
        <div class="div-content-1 page-white">
            <!-- Navigation -->
            @Html.Partial("HeaderPartial")
            <div class="top">
                <div class="container">
                    <div class="landing-heading">
                        @ViewBag.Title
                    </div>
                    <div class="landing-subheading">
                        @ViewBag.Subheading
                    </div>
                </div>
            </div>
            @RenderBody()
            <div class="bottom">
                <div class="container">
                    <h2>
                      ENGAGEMENT MEDIA UNVEILING THE NEW FACE OF MARKETING</h2>
                </div>
            </div>
        </div>
        @Html.Partial("FooterPartial")
    </div>
</body>
</html>
